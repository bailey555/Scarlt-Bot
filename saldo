const { SlashCommandBuilder } = require('discord.js');

module.exports = {
    data: new SlashCommandBuilder()
        .setName('saldo')
        .setDescription('Veja seu saldo atual ðŸ’°'),

    async execute(interaction, db, saveDB) {
        const user = interaction.user.id;

        if (!db[user]) db[user] = { money: 0 };
        saveDB();

        await interaction.reply(`ðŸ’° Seu saldo Ã© **${db[user].money} moedas**!`);
    }
}
